<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <defs>
    <style>
      .bg { fill: #2d3748; }
      .accent { fill: #4299e1; }
      .highlight { fill: #63b3ed; }
      .text { fill: #ffffff; font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif; font-weight: 600; }
    </style>
  </defs>
  
  <!-- Background circle -->
  <circle cx="16" cy="16" r="16" class="bg"/>
  
  <!-- Abstract human figure -->
  <g transform="translate(16, 16)">
    <!-- Head -->
    <circle cx="0" cy="-6" r="3" class="accent"/>
    
    <!-- Body -->
    <rect x="-1.5" y="-3" width="3" height="6" rx="1" class="accent"/>
    
    <!-- Arms -->
    <rect x="-4" y="-2" width="2" height="4" rx="1" class="highlight"/>
    <rect x="2" y="-2" width="2" height="4" rx="1" class="highlight"/>
    
    <!-- Legs -->
    <rect x="-2" y="3" width="1.5" height="4" rx="0.75" class="highlight"/>
    <rect x="0.5" y="3" width="1.5" height="4" rx="0.75" class="highlight"/>
    
    <!-- Tech augmentation elements -->
    <circle cx="-5" cy="-1" r="1" class="text"/>
    <circle cx="5" cy="-1" r="1" class="text"/>
    <line x1="-4" y1="-1" x2="-3" y2="-1" stroke="#ffffff" stroke-width="0.5"/>
    <line x1="3" y1="-1" x2="4" y2="-1" stroke="#ffffff" stroke-width="0.5"/>
  </g>
</svg>
